{
  "name": "puppeteer-project",
  "version": "1.0.0",
  "description": "Automated software testing with Puppeteer",
  "main": "index.js",
  "scripts": {
    "test:example": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./tests/example.test.js",
    "test:device": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./tests/device.test.js",
    "test:e2e": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./tests/e2e/*.test.js"
  },
  "keywords": [
    "Puppeteer",
    "Mocha",
    "Chai",
    "Automation"
  ],
  "author": "KO",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "puppeteer": "^21.6.0"
  }
}
